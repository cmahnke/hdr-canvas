{
  "plugins": ["node_modules/better-docs/typescript"],
  "recurseDepth": 10,
  "source": {
    "include": ["src", "three"],
    "includePattern": "\\.(jsx|js|ts|tsx)$",
    "excludePattern": "(out|node_modules|tests|dist|site|jsdoc)\\/.+"
  },
  "opts": {
    "encoding": "utf8",
    "destination": "tests/site/public/jsdoc",
    "recurse": true,
    "verbose": true,
    "template": "node_modules/better-docs"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "better-docs": {
      "name": "Sample Documentation"
    }
  }
}
